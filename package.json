{
  "type": "module",
  "name": "api-word-list-to-voice-speech",
  "version": "1.0.0",
  "description": "WordListToVoiceSpeech is an API designed to process images containing word lists. It takes an image as input and returns a list with each word along with a synthesized voice corresponding to each word. This functionality is achieved through the integration of Vision AI and Text-to-Speech APIs.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "data": "psql -U username -d database -f app/data/structure-data.sql",
    "test": "jest"
  },
  "keywords": [
    "API"
  ],
  "author": "Aymen El Bakkaly",
  "dependencies": {
    "@google-cloud/text-to-speech": "^5.0.1",
    "@google-cloud/vision": "^4.0.2",
    "@prisma/client": "^5.7.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.1",
    "image-type": "^5.2.0",
    "joi": "^17.6.0",
    "sharp": "^0.33.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "babel-jest": "^29.7.0",
    "debug": "^4.3.4",
    "jest": "^29.7.0",
    "prisma": "^5.7.0"
  }
}
